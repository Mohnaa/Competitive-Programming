using namespace std;
int n,a,b;
signed main(){
  cin>>n;
  while(n--){
    cin>>a>>b;
    if(a!=b) {cout << "Happy Alex\n";
    return 0;}
  }
  cout << "Poor Alex\n";
  return 0;
}
